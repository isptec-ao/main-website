{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "carlos-meneses/laravel-mpdf": "^2.1",
        "davmixcool/laravel-meta-manager": "^1.2",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "highsolutions/eloquent-sequence": "^3.8",
        "inertiajs/inertia-laravel": "^0.3.4",
        "kwn/number-to-words": "^1.12",
        "laravel/framework": "^8.12",
        "laravel/horizon": "^5.6",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.1",
        "reinink/remember-query-strings": "^0.1.0",
        "spatie/browsershot": "^3.41",
        "spatie/laravel-backup": "^6.14",
        "spatie/laravel-medialibrary": "^9.4",
        "spatie/laravel-sitemap": "^5.9",
        "spatie/laravel-tags": "^3.0",
        "spatie/laravel-translatable": "^4.6",
        "spatie/schema-org": "^3.2",
        "thomasjohnkane/snooze": "^1.0",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^0.0.5",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
